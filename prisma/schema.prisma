generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model model {
  idmodel  Int       @id @default(autoincrement())
  name     String?   @db.VarChar(45)
  date     DateTime? @db.DateTime(0)
  moteur   String?   @db.VarChar(45)
  chevaux  String?   @db.VarChar(45)
  histoire String?   @db.VarChar(255)
  marque   marque    @relation(fields: [idmodel], references: [idmarques], onDelete: NoAction, onUpdate: NoAction, map: "idmarques")
}

model marque {
  idmarques Int     @id @default(autoincrement())
  name      String @db.VarChar(45)
  link     String?  @db.VarChar(255)
  model     model?
}
